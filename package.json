    "chai": "^4.3.7",
    "css-loader": "^6.7.1",
    "eslint": "8.33.0",
    "express": "4.18.2",
    "handlebars-loader": "^1.7.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mocha": "^10.2.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.4",
